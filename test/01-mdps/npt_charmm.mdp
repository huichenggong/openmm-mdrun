integrator              = LangevinIntegrator
dt                      = 0.002
nsteps                  = 50000 ; 100ps
nstdcd                  = 0
tau-t                   = 1.0
ref-t                   = 300
gen-vel                 = no

; All of the nonbonded settings should be included in system
pcoupltype              = semiisotropic
tau-p                   = 25 ; frequency in steps, not the same as gmx
ref-p                   = 1.0
surface-tension         = 0.0